<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo1.dao.SysConfigMapper">

    <select id="selectSysConfigAll" resultType="com.example.demo1.entity.SysConfig">
        select * from sys_config
    </select>

    <select id="selectSysConfigByKey" resultType="com.example.demo1.entity.SysConfig">
        select * from sys_config where configkey=#{key}
    </select>

</mapper>